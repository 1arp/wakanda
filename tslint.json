{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-var-requires": false,
    "no-console": false,
    "no-namespace": false,
    "variable-name": ["allow-leading-underscore"]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules/**/*", "dist/**/*"]
  }
}
